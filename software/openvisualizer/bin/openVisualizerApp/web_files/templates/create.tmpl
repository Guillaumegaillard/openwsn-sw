<!DOCTYPE html>
<html>
	<head>
		%include head.tmpl
	</head>
	<body>
		<div id="wrapper">
			%include navbar.tmpl ovVersion=ovVersion
			
			<div id="page-wrapper">
	            <div class="row">
	                <div class="col-lg-12">
	                    <h1 class="page-header">Load a topology file</h1>
	                </div>
	            </div>
                    
	            <div class="row">
	                <div class="col-lg-12">
	                	<h1>Select a topology backup file</h1>
	                	<form method="POST" action="create" enctype="multipart/form-data">
	                		<input name="data" type="file">
	                		<br>
	                		<input type="submit">
	                	</form>
	                	<p> {{result}} </p>
	                </div>
	            </div>
	        </div>
	    </div>
	</body>
</html>